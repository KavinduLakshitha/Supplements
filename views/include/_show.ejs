<% for(var i = 0; i < supplements.length; i++) { %>

<tr>
    <td><%= i + 1 %></td>
    <td><%= supplements[i].name %></td>
    <td><%= supplements[i].price %></td>
    <td><%= supplements[i].category %></td>
    <td>
        <a href="/update-supplement?id=<%= supplements[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fa-regular fa-pen-to-square"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= supplements[i]._id%>>
            <span class="text-gradient"><i class="fa-regular fa-trash-can"></i></span>
        </a>
    </td>
</tr>
<%}%>